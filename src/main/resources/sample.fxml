<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="calc.ui.Controller">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <TextField fx:id="dividend" alignment="CENTER" promptText="dividend">
         <GridPane.margin>
            <Insets left="20.0" />
         </GridPane.margin></TextField>
      <Label fx:id="labelDividend" prefHeight="21.0" prefWidth="91.0" text="Dividend A" visible="true" GridPane.columnIndex="1">
         <GridPane.margin>
            <Insets left="20.0" />
         </GridPane.margin></Label>
      <TextField fx:id="divider" alignment="CENTER" promptText="divider" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets left="20.0" />
         </GridPane.margin></TextField>
      <Label fx:id="labelDivider" prefHeight="21.0" prefWidth="91.0" text="Divider B" visible="true" GridPane.columnIndex="1" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets left="20.0" />
         </GridPane.margin></Label>
      <Button fx:id="toCalculate" mnemonicParsing="false" onAction="#toCalculate" prefHeight="31.0" prefWidth="187.0" text="=" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets left="100.0" />
         </GridPane.margin></Button>
      <TextField fx:id="result" alignment="CENTER" editable="false" promptText="result" GridPane.rowIndex="5">
         <GridPane.margin>
            <Insets left="20.0" />
         </GridPane.margin></TextField>
      <Label fx:id="labelError" prefHeight="21.0" prefWidth="350.0" text="Label" visible="false" GridPane.rowIndex="6">
         <GridPane.margin>
            <Insets left="20.0" />
         </GridPane.margin></Label>
      <Label fx:id="labelDivision" alignment="CENTER" contentDisplay="CENTER" prefHeight="21.0" prefWidth="349.0" text="/" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets left="20.0" />
         </GridPane.margin></Label>
      <Label fx:id="labelResult" prefHeight="21.0" prefWidth="91.0" text="Result C" GridPane.columnIndex="1" GridPane.rowIndex="5">
         <GridPane.margin>
            <Insets left="20.0" />
         </GridPane.margin>
      </Label>
   </children>
   <padding>
      <Insets right="10.0" />
   </padding>
</GridPane>
